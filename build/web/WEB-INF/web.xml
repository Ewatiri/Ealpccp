        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>scholar1</servlet-name>
        <url-pattern>/scholar1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addResource</servlet-name>
        <url-pattern>/addResource</url-pattern>
    </servlet-mapping>
